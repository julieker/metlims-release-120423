
CREATE USER MCHEAR identified by  ru4realmchear
GRANT CREATE TABLE TO MCHEAR
alter user test_user quota unlimited on users;
ALTER USER TEST_USER GRANT CONNECT THROUGH METLIMS;
GRANT CREATE SESSION TO TEST_USER
CREATE USER test_user identified by test_user
   

CREATE USER test_user identified by test_user
  GRANT CREATE SESSION TO MCHEAR
  ALTER USER MCHEAR GRANT CONNECT THROUGH METLIMS;
  GRANT CREATE TABLE TO MCHEAR
  alter user MCHEAR quota unlimited on users;
  
  CREATE TABLE MCHEAR.CORE AS SELECT * FROM METLIMS.CORE
  CREATE TABLE MCHEAR.CORE AS SELECT * FROM METLIMS.CORE WHERE 1 = 2;    
   